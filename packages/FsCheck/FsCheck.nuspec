        <dependency id="FSharp.Core" version="4.1.0"></dependency>
      </group>
      <group targetFramework=".NETStandard1.6">
        <dependency id="NETStandard.Library" version="1.6.1" exclude="Build,Analyzers"></dependency>
        <dependency id="FSharp.Core" version="4.1.17" exclude="Build,Analyzers"></dependency>
      </group>
    </dependencies>
  </metadata>
